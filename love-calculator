print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡

def calculate_love_score(name1, name2):
  true_count = (name1 + name2).lower().count('t') + (name1 + name2).lower().count('r') + (name1 + name2).lower().count('u') + (name1 + name2).lower().count('e')
  love_count = (name1 + name2).lower().count('l') + (name1 + name2).lower().count('o') + (name1 + name2).lower().count('v') + (name1 + name2).lower().count('e')
  combined_score = true_count * 10 + love_count
  return combined_score

def display_message(combined_score):
  if combined_score < 10 or combined_score > 90:
    print(f"Your score is {combined_score}, you go together like coke and mentos.")
  elif 40 <= combined_score <= 50:
    print(f"Your score is {combined_score}, you are alright together.")
  else:
    print(f"Your score is {combined_score}.")

combined_score = calculate_love_score(name1, name2)
display_message(combined_score)

  
  
  

